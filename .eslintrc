{
    "extends": "airbnb",
    "rules": {
        "indent": [2, "tab"],
        "no-mixed-spaces-and-tabs": [2, "smart-tabs"],
        "no-console": 0,
        "prefer-arrow-callback": 0,
        "consistent-return": 0,
        "quote-props": ["error", "as-needed", { "keywords": true, "unnecessary": false }],
        "max-len": ["error", 200],
        "react/jsx-indent-props": [2, "tab"],
    },
    "parser": "babel-eslint"
}
